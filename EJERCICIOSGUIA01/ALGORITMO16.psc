Proceso ALGORITMO16
	//Calcular el área de un rectángulo de lados X e Y.
	
	Definir ladoX Como Real;
	Definir ladoY Como Real;
	Definir area Como Real;
	
	Escribir "Digite el numero del lado X: ";
	leer ladoX;
	
	Escribir "Digite el numero del lado Y: ";
	Leer ladoY;
	
	area <- ladoX*ladoY;
	
	Escribir "El area del rectangulo es: ", area;
	
	
	
FinProceso
