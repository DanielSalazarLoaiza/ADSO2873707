Algoritmo ALGORITMO09
	
	//Hacer un algoritmo que lea el nombre de un estudiante, la cantidad de materias perdidas y la cantidad
	//de materias ganadas.
	
	Definir nombre como cadena;
	Definir materiasPer como entero;
	Definir materiasGan Como Entero;
	
	Escribir "Digite su nombre: ";
	leer nombre;
	
	Escribir "Digite el total de materias perdidas: ";
	leer materiasPer;
	
	Escribir "Digite el total de materias ganadas: ";
	leer materiasGan;
	
FinAlgoritmo
